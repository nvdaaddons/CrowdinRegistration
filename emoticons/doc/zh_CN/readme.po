#, no-wrap
msgid "[[!meta title=\"Emoticons\"]]\n"
msgstr "[[!meta title=\"表情符号插件\"]]\n"

#. type: Bullet: '* '
#, fuzzy
#| msgid ""
#| "Authors: Chris Leo, Noelia Ruiz Martínez, Mesar Hameed, Francisco Javier "
#| "Estrada Martínez"
msgid ""
"Authors: Chris Leo, Noelia Ruiz Martínez, Mesar Hameed, Francisco Javier "
"Estrada Martínez"
msgstr ""
"作者：Chris Leo，NoeliaRuizMartínez，Mesar Hameed，Francisco "
