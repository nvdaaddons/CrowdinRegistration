#. type: Bullet: '* '
msgid "New and updated translations."
msgstr "Traducións novas e actualizadas."

#. type: Bullet: '* '
msgid "Updated NVDA compatibility flags."
msgstr "Actualizadas as marcas de compatibilidade con NVDA."

#. type: Title ###
#, no-wrap
msgid "Version 2.7"
msgstr "Versión 2.7"

#. type: Bullet: '* '
msgid "Updated compatibility flags for recent NVDA versions."
msgstr ""
