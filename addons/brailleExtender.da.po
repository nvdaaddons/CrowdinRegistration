#. type: Plain text
#, no-wrap
msgid "[[!meta title=\"BrailleExtender\"]]\n"
msgstr "[[!meta title=\"Udvidelse til Punktskrift\"]]\n"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Author: André-Abush Clause and contributors"
msgid "Author: André-Abush Clause and contributors"
msgstr "Forfatter: André-Abush klausul og bidragydere"

#. type: Bullet: '* '
msgid "NVDA compatibility: 2019.3 and beyond"
msgstr "NVDA-kompatibilitet: 2019.3 og derefter"

