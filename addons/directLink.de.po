msgid "Author: Fawaz Abdul Rahman"
msgstr "Autor: Fawaz Abdul Rahman"

#. type: Bullet: '* '
msgid "Version: 1.0"
msgstr "Version: 1.0"

#. type: Bullet: '* '
msgid "NVDA compatibility: 2019.3 and beyond"
