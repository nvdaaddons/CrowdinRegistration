msgid "[[!meta title=\"Easy Table Navigator\"]]\n"
msgstr "[[!meta title=\"Simpele Tabelnavigator\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Corentin Bacqu√©-Cazenave, Joseph Lee"
msgstr ""

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr ""

#. type: Bullet: '* '
