#. type: Plain text
#, no-wrap
msgid "> &: 0x26, 38, 0o46, 0b100110  \n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "> and (AMPERSAND [Po])  \n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "> ‚¢ø (1234568)  \n"
