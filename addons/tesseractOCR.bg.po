msgid "[[!meta title=\"TesseractOCR: An OCR add-on\"]]\n"
msgstr "[[!meta title=\"TesseractOCR: Добавка за OCR (оптично разпознаване на символи)\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Rui Fontes, Ângelo Abrantes and Abel Passos do Nascimento Jr."
