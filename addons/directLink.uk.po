msgstr "[[!meta title=\"Direct Link (Пряме посилання)\"]]\n"

#. type: Bullet: '* '
msgid "Author: Fawaz Abdul Rahman"
msgstr "Автор: Fawaz Abdul Rahman"

#. type: Bullet: '* '
msgid "Version: 1.0"
msgstr "Версія: 1.0"

#. type: Bullet: '* '
msgid "NVDA compatibility: 2019.3 and beyond"
msgstr "Сумісність з NVDA: 2019.3 і новіші"

#. type: Bullet: '* '
