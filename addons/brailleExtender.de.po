#. type: Plain text
#, no-wrap
msgid "[[!meta title=\"BrailleExtender\"]]\n"
msgstr "[[!meta title=\"Braille Extender\"]]\n"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Author: André-Abush Clause and contributors"
msgid "Author: André-Abush Clause and contributors"
msgstr "Autor: André-Abush Clause und die NVDA-Community"

#. type: Bullet: '* '
msgid "NVDA compatibility: 2019.3 and beyond"
msgstr "NVDA-Kompatibilität: 2019.3 und neuer"

#. type: Bullet: '* '
