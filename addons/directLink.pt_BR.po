msgstr "[[!meta title=\"Endereço Direto (Direct Link)\"]]\n"

#. type: Bullet: '* '
msgid "Author: Fawaz Abdul Rahman"
msgstr "Autor: Fawaz Abdul Rahman"

#. type: Bullet: '* '
msgid "Version: 1.0"
msgstr "Versão: 1.0"

#. type: Bullet: '* '
msgid "NVDA compatibility: 2019.3 and beyond"
msgstr "Compatibilidade com NVDA: 2019.3 e além"

#. type: Bullet: '* '
