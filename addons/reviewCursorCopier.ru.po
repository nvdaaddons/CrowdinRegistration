msgid "[[!meta title=\"Review Cursor Copier\"]]\n"
msgstr "[[!meta title=\"Копировщик Просмотрового Курсора\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tuukka Ojala <tuukka.ojala@iki.fi>"
msgstr "Автор: Tuukka Ojala <tuukka.ojala@iki.fi>"

#. type: Bullet: '* '
