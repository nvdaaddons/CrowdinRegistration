msgid "[[!meta title=\"Direct Link\"]]\n"
msgstr "[[!meta title=\"Прямая Ссылка\"]]\n"

#. type: Bullet: '* '
msgid "Author: Fawaz Abdul Rahman"
msgstr "Автор: Fawaz Abdul Rahman"

#. type: Bullet: '* '
msgid "Version: 1.0"
msgstr "Версия: 1.0"

#. type: Bullet: '* '
msgid "NVDA compatibility: 2019.3 and beyond"
msgstr "Совместимость с NVDA: 2019.3 и выше"

#. type: Bullet: '* '
