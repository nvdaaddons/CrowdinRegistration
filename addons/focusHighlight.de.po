msgid "[[!meta title=\"Focus Highlight\"]]\n"
msgstr "[[!meta title=\"Focus hervorheben\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Takuya Nishimoto, Karl-Otto Rosenqvist"
msgstr "Authoren: Takuya Nishimoto, Karl-Otto Rosenqvist"

#. type: Bullet: '* '
msgid "Download [stable version][2]"
msgstr "[Stabile Version herunterladen][2]"

#. type: Bullet: '* '
