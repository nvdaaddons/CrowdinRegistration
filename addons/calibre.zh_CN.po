msgid "[[!meta title=\"Calibre\"]]\n"
msgstr "[[!meta title=\"NVDACalibre图书支持插件\"]]\n"

#. type: Bullet: '* '
msgid "Author: Javi Dominguez"
msgstr "作者: Javi Dominguez"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "下载 [稳定版][1]"

#. type: Bullet: '* '
msgid "Download [development version][2]"
